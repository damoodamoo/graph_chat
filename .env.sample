# Azure Configuration (Required)
TF_VAR_subscription_id=your-azure-subscription-id

KAGGLE_API_TOKEN=KGAT_xxxxxxxxxxxxxxxxxxxxxxxxxxx

# Project Configuration (Optional - defaults shown)
TF_VAR_project_name=graphchat
TF_VAR_environment=dev
TF_VAR_location=eastus

# Cosmos DB Configuration (Optional - defaults shown)
TF_VAR_cosmos_consistency_level=Session
TF_VAR_cosmos_throughput=400

# Event Hub Configuration (Optional - defaults shown)
TF_VAR_eventhub_sku=Standard
TF_VAR_eventhub_capacity=1
TF_VAR_eventhub_partition_count=2
TF_VAR_eventhub_message_retention=1

# Azure OpenAI Configuration (Optional - defaults shown)
TF_VAR_openai_sku=S0
TF_VAR_openai_model_name=gpt-4.1
TF_VAR_openai_model_version=2025-04-14
TF_VAR_openai_deployment_capacity=10
